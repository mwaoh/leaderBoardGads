<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mwaoh.leaderboardGads">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_gads_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_gads_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.mwaoh.leaderboardGads.activity.ProjectSubmissionActivity"
            android:parentActivityName="com.mwaoh.leaderboardGads.activity.LeadershipboardActivity">
        </activity>
        <activity android:name="com.mwaoh.leaderboardGads.activity.LeadershipboardActivity"
            android:label="@string/leader_board"/>
        <activity android:name="com.mwaoh.leaderboardGads.activity.LaunchScreenActivity"
            android:theme="@style/Splash">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
    </application>

</manifest>